{
    "name": "stringkey/option-mapper-bundle",
    "description": "Symfony Bundle that allows sets of options to be mapped for one domain in different contexts",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": ["options", "mapper", "symfony", "bundle", "stringkey"],
    "homepage": "https://github.com/stringkey/option-mapper-bundle",
    "autoload": {
        "psr-4": {
            "Stringkey\\OptionMapperBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stringkey\\OptionMapperBundle\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Erik van de Laar",
            "email": "stringkey@wxs.nl"
        }
    ],
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/stringkey/metadata-core-bundle"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-xml":  "*",
        "symfony/config": "^6.4 || ^7.0",
        "symfony/form": "^6.4 || ^7.0",
        "symfony/dependency-injection": "^6.4 || ^7.0",
        "symfony/http-kernel": "^6.4 || ^7.0",
        "symfony/uid": "^6.4 || ^7.0",
        "gedmo/doctrine-extensions": "^3.2",
        "stringkey/metadata-core-bundle": "*@dev",
        "stof/doctrine-extensions-bundle": "^1.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "7.3.x-dev",
        "symfony/test-pack": "dev-main",
        "phpstan/phpstan": "2.1.x-dev",
        "squizlabs/php_codesniffer": "4.0.x-dev",
        "friendsofphp/php-cs-fixer": "dev-master"
    }
}
